!-- TEMPLATES -->
    <template src="Proxy/White_Card_Template.png" default="True">
    <matches>
        <match name="Name" value="Whatever" />
    </matches>
    <textblocks>
        <link block="default_name" separator=" ">
            <property name="Name" />
        </link>
    </textblocks>
    </template>
<!-- Operation Templates -->
    <template src="Proxy/Operations/Operation_Weyland.jpg">
    <matches>
        <match name="Faction" value="The Weyland Consortium" />
        <match name="Type" value="Operation" />
    </matches>
    <textblocks>
        <link block="operation_name" separator=" ">
            <property name="Name" />
        </link>
        <link block="operation_cost" separator=" ">
            <property name="Cost" />
        </link>
        <link block="operation_keywords" separator=" ">
            <property name="Keywords" />
        </link>
        <link block="operation_rules" separator=" ">
            <property name="Rules" />
        </link>
    </textblocks>
    </template>
