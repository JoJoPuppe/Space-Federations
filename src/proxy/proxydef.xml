<!-- SHIP TEMPLATES -->
    <template src="Proxy/proxy_ship.png">
    <matches>
        <match name="Type" value="Ship" />
    </matches>
    <textblocks>
        <link block="ship_text" separator=" ">
            <property name="Text" />
        </link>
        <link block="ship_id" separator=" ">
            <property name="Nick" />
        </link>
        <link block="ship_cost" separator=" ">
            <property name="Cost" />
        </link>
        <link block="ship_att" separator=" ">
            <property name="Attack" />
        </link>
        <link block="ship_def" separator=" ">
            <property name="Defense" />
        </link>
        <link block="ship_int" separator=" ">
