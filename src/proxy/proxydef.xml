<!-- SHIP TEMPLATES -->
    <template src="Proxy/proxy_ship.jpg">
    <matches>
        <match name="Type" value="Ship" />
    </matches>
    <textblocks>
        <link block="ship_text" separator=" ">
            <property name="Text" />
        </link>
        <link block="ship_id" separator=" ">
